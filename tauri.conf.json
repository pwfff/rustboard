{
  "productName": "rustboard",
  "version": "0.1.0",
  "identifier": "com.tauri.dev.rustboard",
  "build": {
    "frontendDist": "ui"
  },
  "app": {
    "enableGTKAppId": true,
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "rustboard",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
